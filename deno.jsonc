{
  "imports": {
    "std/": "https://deno.land/std@0.192.0/",
    "@/": "./"
  },
  "tasks": {
    "dev": "deno run --watch --unstable main.ts",
    "dev:dbg": "deno run --unstable --inspect-brk main.ts",
    "test": "deno test --unstable",
    "test:w": "deno test --unstable --watch",
    "test:cover": "deno test --unstable --coverage=.codecov"
  },
  "fmt": {
    "include": ["./**/*.ts"]
  }
}
